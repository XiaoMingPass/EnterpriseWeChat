    <!-- #################################################################### -->
    <profiles>
        <!-- 开发环境 -->
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <timestampversion>${maven.build.timestamp}</timestampversion>
                <log4j.level>debug</log4j.level>
                <log4j.path>d://wxDataLog/wx/wxmanagerLog-1.0.log</log4j.path>
            </properties>
        </profile>
        <!-- 测试环境 -->
        <profile>
            <id>test</id>
            <properties>
                <timestampversion>${maven.build.timestamp}</timestampversion>
                <log4j.level>info</log4j.level>
                <log4j.path>d://wxDataLog/wx/wxmanagerLog-1.0.log</log4j.path>
            </properties>
        </profile>
        <!-- 生产环境 -->
        <profile>
            <id>prod</id>
            <properties>
                <timestampversion>${maven.build.timestamp}</timestampversion>
                <log4j.level>error</log4j.level>
                <log4j.path>d://wxDataLog/wx/wxmanagerLog-1.0.log</log4j.path>
            </properties>
        </profile>
    </profiles>

